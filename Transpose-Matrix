//Language: C#

public class Solution {
    public int[][] Transpose(int[][] A) {
        int[][] B = new int[A[0].Length][];
        for (int i=0; i<A[0].Length; i++) {
            B[i] = new int[A.Length];
            for (int j=0; j<A.Length; j++) {
                B[i][j] = A[j][i];
            }
        }
        return B;
    }
}

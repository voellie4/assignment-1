//Language: C#

public class Solution {
    public IList<string> FizzBuzz(int n) {
        List<String> A  = new List<String>();
        for (int i = 1; i<=n; i++) {
            if (i%3==0 && i%5==0) A.Add("FizzBuzz");
            else if (i%3==0) A.Add("Fizz");
            else if (i%5==0) A.Add("Buzz");
            else A.Add(i.ToString());
        }
        return A;
    }
}
